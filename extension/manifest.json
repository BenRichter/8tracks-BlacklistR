{
    "name": "BlacklistR - 8tracks",
    "version": "0.1.3",
    "manifest_version": 2,
    "description": "Adds a Button to the player of 8tracks.com to blacklist bad tracks and always skip these songs in every playlist",
    "homepage_url": "https://github.com/BenRichter/8tracks-BlacklistR",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["http://8tracks.com/*", "https://8tracks.com/*"],
            "js": ["js/jquery.min.js","js/inject.js"],
            "css": ["css/style.css"]
        }
    ],
    "permissions": [ "storage" ]
}